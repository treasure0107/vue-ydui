            getProByBudet() {
                if (this.sitename=="其他城市") {
                   this.sitename="";
                }else{
                    this.otherArea=""
                }
                var condBudget;
                if (this.conditionBudget==2) {
                    condBudget=1;
                    this.conditionBudget=1;
                }else{
                    condBudget=2;
                    this.conditionBudget=2;
                }
                this.list=[];
                this.$http.post(this.$api.apihost+'project/getProByChoose', {
                    conditionArea:this.sitename,
                    conditionBudget:condBudget,
                    conditionDistanc:1,
                    otherArea:this.otherArea,
                    page: this.page,
                    pagesize: this.pageSize
                },{emulateJSON:true}).then( (res)=> {
                    const _list = res.body.obj.projectList;

                    this.list = [...this.list, ..._list];

                    if (_list.length < this.pageSize || this.page == 3) {
                        /* 所有数据加载完毕 */
                        this.$refs.infinitescrollDemo.$emit('ydui.infinitescroll.loadedDone');
                        return;
                    }
                    /* 单次请求数据完毕 */
                    this.$refs.infinitescrollDemo.$emit('ydui.infinitescroll.finishLoad');
                    this.page++;
                },(err)=>{

                });
            },
            getProByChoose() {
                if (this.sitename=="其他城市") {
                   this.sitename="";
                }else{
                    this.otherArea=""
                }
                this.$http.post(this.$api.apihost+'project/getProByChoose', {
                    conditionArea:this.sitename,
                    conditionBudget:2,
                    conditionDistanc:1,
                    otherArea:this.otherArea,
                    page: this.page,
                    pagesize: this.pageSize
                },{emulateJSON:true}).then( (res)=> {
                    const _list = res.body.obj.projectList;

                    this.list = [...this.list, ..._list];

                    if (_list.length < this.pageSize || this.page == 3) {
                        /* 所有数据加载完毕 */
                        this.$refs.infinitescrollDemo.$emit('ydui.infinitescroll.loadedDone');
                        return;
                    }
                    /* 单次请求数据完毕 */
                    this.$refs.infinitescrollDemo.$emit('ydui.infinitescroll.finishLoad');
                    this.page++;
                },(err)=>{

                });
            }
        //     getDistance(){
